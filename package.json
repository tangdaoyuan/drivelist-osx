{
  "name": "@suger-tdy/drivelits-osx",
  "version": "0.0.0",
  "packageManager": "pnpm@7.5.0",
  "description": "Get driver list in osx",
  "keywords": [
    "drivelits-osx"
  ],
  "license": "Apache-2.0",
  "author": "tangdaoyuan <1197633750@qq.com> (https://github.com/tangdaoyuan/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangdaoyuan/unplugin-mock.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsx ./src/index.ts",
    "play": "npm -C playground run dev",
    "stub": "unbuild --stub",
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --tag --commit --push && pnpm publish",
    "build:lib": "node-gyp rebuild"
  },
  "bugs": {
    "url": "https://github.com/tangdaoyuan/drivelist-osx/issues"
  },
  "homepage": "https://github.com/tangdaoyuan/drivelist-osx#readme",
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^5.0.0",
    "picocolors": "^1.0.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@suger-tdy/eslint-config": "1.1.6",
    "@types/bindings": "^1.5.1",
    "@types/yargs": "^17.0.10",
    "bumpp": "8.2.1",
    "esbuild": "0.14.48",
    "eslint": "8.19.0",
    "tsx": "3.7.1",
    "typescript": "4.7.4",
    "unbuild": "0.7.4",
    "vite": "2.9.13",
    "vitest": "0.17.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
